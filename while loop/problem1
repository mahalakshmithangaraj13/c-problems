#include<stdio.h>
int main(){
    int n,noises;
    scanf("%d",&n);
    int i=1;
    int count=0,cur_max=0,max=0;
    while(i<=n){
        scanf("%d",&noises);
        if(noises>70){
           count++;
           cur_max++;
           if (cur_max>max){
            max=cur_max;
           }
        }
        else{
            cur_max=0;
        }
        i++;
    }
    printf("Noise Violations: %d\n",count);
    printf("Longest Violation Streak: %d",max);
    return 0;
}
